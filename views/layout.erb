    /* Styles for Accordion */
    .toggle:last-child {
      border-bottom: 1px solid #ddd;
    }
    .toggle .toggle-title {
      position: relative;
      display: block;
      border-top: 1px solid #ddd;
      margin-bottom: 6px;
    }
    .toggle .toggle-title h3 {
      font-size: 20px;
      margin: 0px;
      line-height: 1;
      cursor: pointer;
      font-weight: 200;
    }
    .toggle .toggle-inner {
      padding: 7px 25px 10px 25px;
